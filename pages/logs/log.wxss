/*当前页面的样式*/
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgba(184, 233, 171, 0.651);
}
.avatarUrl{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100rpx auto;
}
button{
  width: 200rpx;
  height: 200rpx;
  font-size: 22rpx;
  border-radius: 50%;
  margin: 100rpx auto;
  text-align: center;
  line-height: 200rpx;  
  background: rgba(228, 255, 109, 0.877)
}
.userName{
  font-size: 20rpx;
  margin: 100rpx 0;
}
.goStudy{
  width: 400rpx;
  height: 100rpx;
  border: 1px solid black;
  border-radius: 10rpx;
  text-align: center;
  line-height: 100rpx;
}